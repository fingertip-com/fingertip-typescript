{
  "dxt_version": "0.1",
  "name": "fingertip-mcp",
  "version": "0.73.0",
  "description": "The official MCP Server for the Fingertip API",
  "author": {
    "name": "Fingertip",
    "email": "hello@fingertip.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fingertip-com/fingertip-typescript.git"
  },
  "homepage": "https://github.com/fingertip-com/fingertip-typescript/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.fingertip.com",
  "server": {
    "type": "node",
    "entry_point": "${__dirname}/index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "FINGERTIP_API_KEY": "${user_config.FINGERTIP_API_KEY}"
      }
    }
  },
  "user_config": {
    "FINGERTIP_API_KEY": {
      "title": "api_key",
      "description": "Fingertip API authentication using Bearer token",
      "required": true,
      "type": "string"
    }
  },
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
